   "source": [
    "\n",
    "def simple_decorator(func):\n",
    "    def wrapper():\n",
    "        print(\"До вызова функции\")\n",
    "        func()\n",
    "        print(\"После вызова функции\")\n",
    "    return wrapper\n",
    "\n",
    "@simple_decorator\n",
    "def say_hello():\n",
    "    print(\"Привет!\")\n",
    "\n",
    "# Вызов функции\n",
    "say_hello()\n"
   ]
